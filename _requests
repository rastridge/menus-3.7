RewriteCond %{QUERY_STRING} /id=([0-9]*)
/site/stats/detail.php$ /schedule/page/%1? [R,L]
#
/site/stats$ /schedule 
#
/buffalogallery/v /photos 
#
/v-web/gallery /photos 
#
/gallery3/index.php /photos 
#
/site/newsletters/newsletters_archive.php /archive/newsletters  
#
RewriteCond %{QUERY_STRING} /id=(\d*[/&])$
/site/news/detail.php /archive/news/page/%1? [R,L]
#
RewriteCond %{QUERY_STRING} /index=(\d*[/&])
/site/news/detail_for_FB.php /archive/news/page/%1? [R,L]
#
/site/news/news_archive.php /archive/news  
#
/archive$ /archive/documents  
#
